    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.12"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
